{
    "env": {
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "promise/param-names": "off",
        "eqeqeq": "off",
        "no-mixed-operators": "off"
    }
}
